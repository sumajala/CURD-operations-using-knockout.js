@model knockoutCURD.Models.Store



@{
    ViewBag.Title = "Details";
}
<h2>Details</h2>
<div id="store-details">
    <h4>Store</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.StoreID)</dt>
        <dd data-bind="text: selectedStore.StoreID"></dd>
        <dt>@Html.DisplayNameFor(model => model.Name)</dt>
        <dd data-bind="text: selectedStore.Name"></dd>
        <dt>@Html.DisplayNameFor(model => model.Address)</dt>
        <dd data-bind="text: selectedStore.Address"></dd>
    </dl>
</div>